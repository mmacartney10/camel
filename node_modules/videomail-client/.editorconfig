# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4

[**.js, **.json]
spaces_around_operators = true
space_after_control_statements = true
spaces_in_brackets = false
